<head>
<style>
table.buttons tr td {
  text-align:center;
}
.button {
height:50px; 
width:100%;
font-weight:bold;
}
.red {
background: red;
}
.green {
background: green;
}
.yellow {
background: yellow;
}
</style>
</head>
<table width="100%" class="buttons">
  <tr>
    <td>
      <% if counter > 0 %>
          <form action="/<%= counter - 1 %>">
            <button class="button">&laquo;</button>
          </form>
      <% end %>
    </td>
    <td>
      <form action="/u1/<%= counter %>" method="post">
        <button class="button green">User 1</button>
      </form>
    </td>

    <td>
      <form action="/set/<%= counter %>" method="post">
        <input type="text" name="cyc_name" style="width: 70%; height: 50px;" onkeyup="document.getElementById('set').disabled=false" onclick="document.getElementById('set').disabled=false">
        <input type="submit" id="set" disabled="true" value="Set" style="width: 20%; height: 50px;">
      </form>
    </td>
    <td>
      <form action="/u2/<%= counter %>" method="post">
        <button class="button green">User 2</button>
      </form>
    </td>
    <td>
      <% if counter < 599 %>
          <form action="/<%= counter + 1 %>">
            <button class="button">&raquo;</button>
          </form>
      <% end %>
    </td>
  </tr>
</table>
<hr>
<table width="100%">
  <tr>
    <td style="text-align:center" width="33%"><h1><%= cyc1 %></h1></td>
    <td style="text-align:center" width="33%"><h1><%= wiki %></h1></td>
    <td style="text-align:center" width="33%"><h1><%= cyc2 %></h1></td>
  </tr>
  <tr height="100%">
    <td style="text-align:center" width="33%" height="500px"><iframe src="<%= cyc_url1 %>" align="left" height="100%" width="100%"></iframe></td>
    <td style="text-align:center" width="33%" height="500px"><iframe src="<%= wiki_url %>" align="center" height="100%" width="100%"></iframe></td>
    <td style="text-align:center" width="33%" height="500px"><iframe src="<%= cyc_url2 %>" align="right" height="100%" width="100%"></iframe></td>
  </tr>
</table>

